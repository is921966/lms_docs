# TestFlight Feedback Configuration
# Copy this file to .env.local and fill in your values

# App Store Connect API Credentials
APP_STORE_CONNECT_API_KEY_ID=XXXXXXXXXX
APP_STORE_CONNECT_API_ISSUER_ID=XXXXXXXX-XXXX-XXXX-XXXX-XXXXXXXXXXXX
APP_STORE_CONNECT_API_KEY_PATH=./private_keys/AuthKey_XXXXXXXXXX.p8

# Your App ID in App Store Connect
APP_ID=1234567890

# Optional: GitHub Integration
GITHUB_TOKEN=ghp_xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
GITHUB_REPOSITORY=owner/repo

# Optional: Slack Integration
SLACK_WEBHOOK=https://hooks.slack.com/services/XXXXXXXXX/XXXXXXXXX/xxxxxxxxxxxxxxxxxxxxxxxx

# Optional: Feedback fetch period (days)
FEEDBACK_DAYS=7 